{
  "name": "bem-levels",
  "main": "./lib/bem-levels",
  "version": "0.2.1",
  "description": "Search and open BEM blocks and elements in order of levels.",
  "keywords": [
    "bem",
    "open"
  ],
  "configSchema": {
    "^[\\w-]+$": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "uniqueItems": true
    }
  },
  "repository": "https://github.com/tenorok/atom-bem-levels",
  "bugs": {
    "url": "https://github.com/tenorok/atom-bem-levels/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "file-icons.element-icons": {
      "versions": {
        "1.0.0": "consumeElementIcons"
      }
    }
  },
  "package-deps": [
    "local-config"
  ],
  "dependencies": {
    "@bem/entity-name": "^1.3.1",
    "@bem/naming": "^2.0.0-3",
    "@bem/walk": "^1.0.0-2",
    "atom-space-pen-views": "^2.2.0"
  },
  "atomTestRunner": "./test-runner",
  "devDependencies": {
    "atom-mocha-test-runner": "^1.0.0",
    "chai": "^3.5.0",
    "flow-bin": "^0.39.0",
    "fs-plus": "^2.9.3",
    "git-hooks": "^1.1.8",
    "sinon": "^1.17.7"
  }
}
